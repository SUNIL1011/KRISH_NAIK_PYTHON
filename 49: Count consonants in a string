def count_consonants(s):
    """
    Function to count the number of consonants in the input string.
    
    Parameters:
    s (str): The input string to check for consonants.
    
    Returns:
    int: The count of consonants in the input string.
    """
    # Your code here
    count = 0
    vowels ="aeiouAEIOU"
    for i in s:
        if ('a' <= i <= 'z') or ('A' <= i <= 'Z'):
           if i not in vowels:
              count += 1
    return count
